{
	"name": "@mrksbnc/complib",
	"version": "0.0.0",
	"description": "Reusable component collection built with Vite and Vue 3.",
	"scripts": {
		"dev": "vite",
		"build": "npm run type-check && npm run build-only",
		"preview": "vite preview",
		"test:unit": "vitest",
		"build-only": "vite build",
		"type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
		"lint": "eslint . --ext .vue,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write src/",
		"test": "vitest",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build -o docs",
		"release": "npm run build && npm publish",
		"deploy": "npm run build-storybook && gh-pages -d docs"
	},
	"homepage": "http://mrksbnc.github.io/complib",
	"repository": {
		"type": "git",
		"url": "https://github.com/mrksbnc/complib.git"
	},
	"files": [
		"dist/**/*"
	],
	"main": "./dist/complib.umd.js",
	"module": "./dist/complib.es.js",
	"exports": {
		".": {
			"import": "./dist/complib.es.js",
			"require": "./dist/complib.umd.js"
		},
		"./index.css": {
			"import": "./dist/index.css",
			"require": "./dist/index.css"
		}
	},
	"types": "./dist/index.d.ts",
	"dependencies": {
		"vue": "^3.3.4"
	},
	"devDependencies": {
		"@storybook/addon-a11y": "^7.2.2",
		"@storybook/addon-actions": "^7.2.3",
		"@storybook/addon-controls": "^7.2.3",
		"@storybook/addon-essentials": "^7.2.2",
		"@storybook/addon-interactions": "^7.2.2",
		"@storybook/addon-links": "^7.2.3",
		"@storybook/addon-styling": "^1.3.6",
		"@storybook/addon-viewport": "^7.2.2",
		"@storybook/addons": "^7.2.2",
		"@storybook/blocks": "^7.2.2",
		"@storybook/manager-api": "^7.2.2",
		"@storybook/testing-library": "^0.2.0",
		"@storybook/testing-vue3": "^1.0.0",
		"@storybook/theming": "^7.2.2",
		"@storybook/vue3": "^7.2.2",
		"@storybook/vue3-vite": "^7.2.2",
		"@testing-library/vue": "^7.0.0",
		"@tsconfig/node18": "^18.2.0",
		"@types/jsdom": "^21.1.1",
		"@types/node": "^18.17.0",
		"@typescript-eslint/eslint-plugin": "^6.3.0",
		"@typescript-eslint/parser": "^6.3.0",
		"@vitejs/plugin-vue": "^4.2.3",
		"@vitest/coverage-v8": "^0.34.1",
		"@vue/eslint-config-typescript": "^11.0.3",
		"@vue/test-utils": "^2.4.1",
		"@vue/tsconfig": "^0.4.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-storybook": "^0.6.13",
		"eslint-plugin-vue": "^9.16.1",
		"gh-pages": "^6.0.0",
		"jsdom": "^22.1.0",
		"postcss": "^8.4.27",
		"prettier": "^3.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sass": "^1.65.1",
		"storybook": "^7.2.3",
		"tailwindcss": "^3.3.3",
		"typescript": "~5.1.6",
		"vite": "^4.4.9",
		"vite-plugin-dts": "^3.5.2",
		"vitest": "^0.34.1",
		"vue-tsc": "^1.8.8"
	}
}
